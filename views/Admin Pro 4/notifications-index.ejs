      <div class="modal fade" id="myModal" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title">Reply</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <i class="material-icons">clear</i>
              </button>
            </div>
            <div class="modal-body">
              <form class="contact-form">
                <div class="row" style="padding-bottom: 10px;">
                  <div class="col-md-6">
                    <div class="form-group">
                      <label class="bmd-label-floating">Subject</label>
                      <input type="text" required="required" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6 space-out modal-date-time">
                    <div class="form-group">
                      <label class="label-control">Suited Date & Time</label>
                      <input type="text" class="form-control datetimepicker" value="">
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label for="exampleMessage" class="bmd-label-floating">Description</label>
                  <textarea type="text" class="form-control" rows="3" id="exampleMessage"></textarea>
                </div>
              </form>
            </div>
            <div class="modal-footer">
              <!--<button type="button" class="btn btn-primary btn-link">Send</button>-->
              <button type="button" class="btn btn-primary btn-link" onclick='swal({ title:"Success!", text: "Message Sent!", type: "success", buttonsStyling: false, confirmButtonClass: "btn btn-success"})'>Send</button>
              <button type="button" class="btn btn-danger btn-link" data-dismiss="modal">Close</button>
            </div>
          </div>
        </div>
      </div>
