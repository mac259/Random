  <div class="page-header header-filter" style="background-image: url('/Student Pro 2/assets/img/bg-login.jpeg'); background-size: cover; background-position: top center; height: 100vh;">
    <div class="container">
      <div class="row">
        <div class="col-lg-4 col-md-6 ml-auto mr-auto">
          <div class="card card-login">
            <form class="form" method="post" action="/student_login">
              <div class="card-header card-header-primary text-center">
                <i class="material-icons" style="font-size: 3.1rem;">fingerprint</i>
                <h4 class="card-title">Login</h4>
              </div>
              <p class="description text-center">Contact admin for details</p>
              <div class="card-body">
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text">
                      <i class="material-icons">face</i>
                    </span>
                  </div>
                  <input type="text" class="form-control" name="id" placeholder="Enter Username" required>
                </div>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text">
                      <i class="material-icons">lock_outline</i>
                    </span>
                  </div>
                  <input id="loginPassStatus" type="password"  name="password" class="form-control" placeholder="Enter Password" required>
                  <div class="toggle-pass">
                    <div class="buttons-pass" onclick="this.classList.toggle('active'); changeView();">
                      <div class="circle-pass">
                      </div>
                    </div>
                  </div>
                </div>
                <div class="input-group cont" style="padding-left: 22px;">
                  
                    <div class="g-recaptcha" id ="gg" data-theme="light" data-sitekey="6Lfg-l4UAAAAAALvo7I0X50fbj6gbMUUK9nQwCAh"></div>
                  
                </div>
              </div>
              <div class="footer text-center">
                <input type = 'submit' class="btn btn-primary btn-link btn-wd btn-lg" value = 'Get Started' name="submit">
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <footer class="footer footer-default">
      <div class="container">
